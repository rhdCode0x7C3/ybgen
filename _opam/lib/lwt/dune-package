(library
 (name lwt)
 (kind normal)
 (synopsis "Monadic promises and concurrent I/O")
 (archives (byte lwt.cma) (native lwt.cmxa))
 (plugins (byte lwt.cma) (native lwt.cmxs))
 (native_archives lwt.a)
 (modes byte native)
 (modules
  (unwrapped
   (module
